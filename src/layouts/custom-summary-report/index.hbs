{{> sideNav }}
<!-- [ Main Content ] start -->
<div class="pcoded-main-container">
  <div class="pcoded-content">
    <!-- [ Main Content ] start -->
    <div class="p-0">
      <div class="p-0 contant-holder">
        <!-- contant holder start -->
        <h3 class="main-head-3">Summay Report</h3>
        <!-- contant holder end -->
        <div class="col-md-12 pt-2 pb-5">
          <main>
            <div class="frm-border">
              <form id="controls-form">
                <div class="row">

                  <div class="col-md-3 adminName">
                    <label for="adminName">Admin name</label>
                    <select type="text" class="form-control select2 dropdown" id="adminName" name="adminName">
                      <option value="">-- Select --</option>
                    </select>
                  </div>

                  <div class="col-md-3 resellerName">
                    <label for="resellerName">Reseller name</label>
                    <select type="text" class="form-control select2 dropdown" id="resellerName" name="resellerName">
                      <option value="">-- Select --</option>
                    </select>
                  </div>

                  <div class="col-md-3 sellerName">
                    <label for="sellerName">Seller name</label>
                    <select type="text" class="form-control select2 dropdown" id="sellerName" name="sellerName">
                      <option value="">-- Select --</option>
                    </select>
                  </div>

                  <div class="col-md-3 clientName">
                    <label for="clientName">Client name</label>
                    <select type="text" class="form-control select2 dropdown" id="clientName" name="clientName">
                      <option value="">-- Select --</option>
                    </select>
                  </div>

                  <div class="col-md-3">
                    <div class="form-group">
                      <label for="from">From</label>
                      <input type="text" class="form-control datepicker" id="from" name="fromDate"
                        aria-describedby="add-sender-id" placeholder="Choose Date" required>
                    </div>
                  </div>
                  <div class="col-md-3">
                    <div class="form-group">
                      <label for="to">To</label>
                      <input type="text" class="form-control datepicker" id="to" name="toDate"
                        aria-describedby="add-sender-id" placeholder="Choose Date" required>
                    </div>
                  </div>
                  <div class="col-md-3">
                    <div class="form-group mt-4 pt-2">
                      <input type="submit" value="Search" class="btn btn-primary">
                      <input type="reset" value="Reset" class="btn btn-danger" />
                    </div>
                  </div>
                  <div class="col-md-2">
                    <div class="form-group mt-4 pt-2 ">
                     <button class="btn btn-info d-none float-left " id="donwloadFileButton" ></button>
                    </div>
                  </div>

		                   </div>
              </form>
            </div>
          </main>

          <table id="misTable" class="table table-striped table-sm table-bordered dt-responsive nowrap"
            style="width:100%">
            <tfoot>
                        <tr>
                          <td class="Total"></td>
                          <td></td>
                          <td></td>
                          <td></td>
                          <td></td>
                          <td></td>
                          <td></td>
                          <td></td>
                        </tr>
            </tfoot>
            </table>
          <ul id="misPagination" class="pagination float-right table-pagination">
            <li class="page-item"><a class="page-link" id="pagePrev" href="#">Previous</a></li>
            <li class="page-item"><a class="page-link" id="pageNext" href="#">Next</a></li>
          </ul>
        </div>
      </div>
      <!-- [ Main Content ] end -->
    </div>

  </div>
