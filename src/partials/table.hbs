        <button type="button" name="TRANS_PROMO_DND" class="btn btn-outline-primary btn-sm editModal" href="#">
          <i class="fa fa-pencil"></i>
        </button> &nbsp;&nbsp;&nbsp;
        <button type="button" class="btn btn-outline-danger btn-sm deleteRow">
          <i class="fa fa-trash"></i>
        </button>
      </td>
    </tr>
    {{/each}}
  </tbody>

  <tr class="routingTh">
    <td colspan="4">
      <label for="TRANS_PROMO_NON_DND"><i class="fa fa-minus-square"
          aria-hidden="true"></i>&nbsp;&nbsp;TRANS_PROMO_NON_DND</label>
      <input type="checkbox" name="TRANS_PROMO_NON_DND" id="TRANS_PROMO_NON_DND" data-toggle="toggle">
    </td>
    <td><button type="button" name="TRANS_PROMO_NON_DND" class="btn btn-info btn-sm addModal" href="#"><i
          class="fa fa-plus"></i> Add</button> &nbsp;&nbsp;&nbsp;<button type="button" name="TRANS_PROMO_NON_DND"
        class="btn btn-success btn-sm saveRouting"><i class="fa fa-save"></i> Save</button></td>
  </tr>
  <tbody class="hide">
    {{#each transPromoNonDndRoutingData }}
    <tr class="TRANS_PROMO_NON_DND routingRow">
      {{#each this }}
      <td data-id="0">{{ this }}</td>
      {{/each}}
