{{> sideNav }}
<!-- [ Main Content ] start -->
<div class="pcoded-main-container">
  <div class="pcoded-content">
    <!-- [ Main Content ] start -->
    <div class="p-0">
      <div class="p-0 contant-holder">
        <!-- contant holder start -->
        <h3 class="main-head-3">MIS</h3>
        <!-- contant holder end -->
        <div class="col-md-12 pt-2 pb-5">
          <main>
            <div class="frm-border">
              <form id="controls-form">
                <div class="row">
                  <div class="col-md-3 adminName">
                    <label for="adminName">Admin name</label>
                    <select type="text" class="form-control select2 dropdown" id="adminName" name="adminName">
                      <option value="">-- Select --</option>
                    </select>
                  </div>

                  <div class="col-md-3 resellerName">
                    <label for="resellerName">Reseller name</label>
                    <select type="text" class="form-control select2 dropdown" id="resellerName" name="resellerName">
                      <option value="">-- Select --</option>
                    </select>
                  </div>

                  <div class="col-md-3 sellerName">
                    <label for="sellerName">Seller name</label>
                    <select type="text" class="form-control select2 dropdown" id="sellerName" name="sellerName">
                      <option value="">-- Select --</option>
                    </select>
                  </div>

                  <div class="col-md-3 clientName">
                    <label for="clientName">Client name</label>
                    <select type="text" class="form-control select2 dropdown" id="clientName" name="clientName">
                      <option value="">-- Select --</option>
                    </select>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-3">
                    <div class="form-group">
                      <label for="mobile">Mobile Number</label>
                      <div class="input-group mb-3">
                        <div class="input-group-prepend">
                          <span class="input-group-text" id="basic-addon1">91</span>
                        </div>
                        <input type="text" class="form-control" id="mobileNumber" name="mobileNumber" pattern="\d{10}">
                      </div>
                    </div>
                  </div>

                  <div class="col-md-2">
                    <div class="form-group  pt-1">
                      <label for="fromDate">From</label>
                      <input type="text" class="form-control datepicker" id="from" name="fromDate" required>
                    </div>
                  </div>
                  <div class="col-md-2">
                    <div class="form-group pt-1">
                      <label for="toDate">To</label>
                      <input type="text" class="form-control datepicker" id="to" name="toDate" required>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-group mt-4 pt-2">
                      <input type="submit" value="Search" class="btn btn-primary">
                      <input type="reset" value="Reset" class="btn btn-danger" />
                      <button class="btn btn-info d-none float-right" id="donwloadFileButton"></button>
                    </div>
                  </div>
                </div>
            </div>
            </form>
            <input type="hidden" id="pageNumber" value="1" />
            <input type="hidden" id="totalPages" />

            <table id="misTable" class="table table-striped table-sm table-bordered dt-responsive nowrap"
              style="width:100%"></table>
              <br/>
            <ul id="misPagination" class="pagination float-right table-pagination">
              <li class="page-item"><a class="page-link" id="pagePrev" href="#">Previous</a></li>
              <li class="page-item"><a class="page-link" id="pageNext" href="#">Next</a></li>
            </ul>

        </div>
        </main>
      </div>
    </div>
    <!-- [ Main Content ] end -->
  </div>

</div>
<div class="footer-div">
  <span class="copy float-left">
    <p id="demo"></p> &copy; All Rights Reserved
  </span>
  <span class="float-right">
    <ul class="social-media-icon">
      <li><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
      <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
      <li><a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
      <li><a href="#"><i class="fa fa-rss" aria-hidden="true"></i></a></li>
      <li><a href="#"><i class="fa fa-whatsapp" aria-hidden="true"></i></a></li>
    </ul>
  </span>
</div>
</div>
