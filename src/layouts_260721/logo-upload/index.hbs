  <div class="pcoded-content">
    <!-- [ Main Content ] start -->
    <div class="p-0">
      <div class="p-0 contant-holder">
        <!-- contant holder start -->
        <h3 class="main-head-3">Logo Upload</h3>
        <!-- contant holder end -->
        <div class="col-md-12 pt-2 pb-5">
          <main>
            <div class="frm-border">
              <form id="controls-form">
                <div class="row">
                  <label for="selectFile" class="col-md-2 col-form-label">Select File</label>
                  <div class="col-md-4">
                    <div class="custom-file">
                      <input type="file" class="custom-file-input" id="selectFile" name="selectFile"
                        accept=".png,.jpg,.jpeg">
                      <label class="custom-file-label" for="customFile">Choose file</label>
                    </div>
<small><b>Note:</b> Only jpeg, jpg, png, will be allowed </small>
                  </div>
                  <div class="col-md-2">
                    <button type="button" id="fileUpload" class="btn btn-primary">Upload</button>
                  </div>
                </div>
              </form>
            </div>
          </main>
        </div>
      </div>
      <!-- [ Main Content ] end -->
    </div>

