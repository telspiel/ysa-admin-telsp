{{> sideNav this }}
<!-- [ Main Content ] start -->
<div class="pcoded-main-container">
  <div class="pcoded-content">
    <!-- [ Main Content ] start -->
    <div class="p-0">
      <div class="p-0 contant-holder">
        <!-- contant holder start -->
        <h3 class="main-head-3">Operator Summary</h3>
        <!-- contant holder end -->
        <div class="col-md-12 pt-2 pb-5">
          <main class="px-4 py-3">
            <div id="alert-box" class="alert-box"></div>
            <nav>
              <div class="nav nav-tabs" id="nav-tab" role="tablist">
                <a class="nav-item nav-link active" id="connect-summary-tab" data-toggle="tab" href="#connectSummary"
                  role="tab" aria-controls="approve" aria-selected="false">Connect Summary</a>
                <a class="nav-item nav-link" id="senderId-traffic-tab" data-toggle="tab" href="#senderIdTraffic"
                  role="tab" aria-controls="pending" aria-selected="false">SenderId Wise Traffic</a>
              </div>
            </nav>
            <div class="tab-content" id="nav-tabContent">
              <div class="tab-pane active" id="connectSummary" role="tabpanel" aria-labelledby="nav-home-tab">
                <br>
                <br>
                <div class="col-md-12 ">
                  <div class="row accordation-header active" id="todayTableHeader">
                    <div class="col-md-6 accordation-title">
                      <h6>Today's Summary</h6>
                    </div>
                    <div class="col-md-6 todayTableDisplay actions d-none">
                      <h4 class="float-right"><i class="fa fa-plus-circle" aria-hidden="true"></i></h4>
                    </div>
                    <div class="col-md-6 todayTableDisplay actions">
                      <h4 class="float-right"><i class="fa fa-minus-circle" aria-hidden="true"></i></h4>
                      <span id="downloadTodayReport"></span>
                    </div>
                  </div>
                  <div class="row todayTableDisplay">
                    <table id="todaysTable" class="table table-striped table-sm table-bordered dt-responsive nowrap"
                      style="width:100%">
                      <tfoot>
                        <tr>
                          <th colspan="2" style="text-align:right">Total:</th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                        </tr>
                      </tfoot>
                    </table>
                  </div>
                </div>
                <div class="col-md-12">
                  <div class="row accordation-header" id="yesterDayTableHeader">
                    <div class="col-md-6 accordation-title">
                      <h6>Yesterday's Summary</h6>
                    </div>
                    <div class="col-md-6 yesterDayTableDisplay actions">
                      <h4 class="float-right"><i class="fa fa-plus-circle" aria-hidden="true"></i></h4>
                    </div>
                    <div class="col-md-6 yesterDayTableDisplay actions d-none">
                      <h4 class="float-right"><i class="fa fa-minus-circle" aria-hidden="true"></i></h4>
                      <span id="downloadPreviousDayReport"></span>
                    </div>
                  </div>
                  <div class="row yesterDayTableDisplay d-none">
                    <table id="yesterDayTable" class="table table-striped table-sm table-bordered dt-responsive nowrap"
                      style="width:100%">
                      <tfoot>
                        <tr>
                          <th colspan="2" style="text-align:right">Total:</th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                        </tr>
                      </tfoot>
                      </table>
                  </div>
                </div>
                <div class="col-md-12 ">
                  <div class="row accordation-header" id="currentMonthTableHeader">
                    <div class="col-md-6 accordation-title">
                      <h6>This Month's Summary</h6>
                    </div>
                    <div class="col-md-6 currentMonthTableDisplay actions">
                      <h4 class="float-right"><i class="fa fa-plus-circle" aria-hidden="true"></i></h4>
                    </div>
                    <div class="col-md-6 currentMonthTableDisplay actions d-none">
                      <h4 class="float-right"><i class="fa fa-minus-circle" aria-hidden="true"></i></h4>
                      <span id="currentMonthTableDownload"></span>
                    </div>
                  </div>

                  <div class="row currentMonthTableDisplay d-none">
                    <table id="currentMonthTable"
                      class="table table-striped table-sm table-bordered dt-responsive nowrap" style="width:100%">
                      <tfoot>
                        <tr>
                          <th colspan="2" style="text-align:right">Total:</th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                        </tr>
                      </tfoot>
                    </table>
                  </div>
                </div>
                <div class="col-md-12 ">
                  <div class="row accordation-header" id="lastMonthTableHeader">
                    <div class="col-md-6 accordation-title">
                      <h6>Last Month's Summary</h6>
                    </div>
                    <div class="col-md-6 lastMonthTableDisplay actions">
                      <h4 class="float-right"><i class="fa fa-plus-circle" aria-hidden="true"></i></h4>
                    </div>
                    <div class="col-md-6 lastMonthTableDisplay actions d-none">
                      <h4 class="float-right"><i class="fa fa-minus-circle" aria-hidden="true"></i></h4>
                      <span id="downloadPreviousMonthReport"></span>
                    </div>
                  </div>

                  <div class="row lastMonthTableDisplay d-none">
                    <table id="lastMonthTable" class="table table-striped table-sm table-bordered dt-responsive nowrap"
                      style="width:100%">
                      <tfoot>
                        <tr>
                          <th colspan="2" style="text-align:right">Total:</th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                          <th colspan="1"></th>
                        </tr>
                      </tfoot>
                      </table>
                  </div>
                </div>
              </div>
              <div class="tab-pane fade" id="senderIdTraffic" role="tabpanel" aria-labelledby="nav-profile-tab">
                <br>
                <div class="frm-border">
                  <form id="controls-form" action="#">
                    <div class="row">
                      <div class="col-md-3">
                        <div class="form-group">
                          <label for="from">From</label>
                          <input type="text" class="form-control datepicker" id="from" name="from"
                            aria-describedby="add-sender-id" placeholder="Choose Date" required>
                        </div>
                      </div>
                      <div class="col-md-3">
                        <div class="form-group">
                          <label for="to">To</label>
                          <input type="text" class="form-control datepicker" id="to" name="to"
                            aria-describedby="add-sender-id" placeholder="Choose Date" required>
                        </div>
                      </div>
                      <div class="col-md-1">
                        <div class="form-group mt-4 pt-1">
                          <input type="submit" value="Search" class="btn btn-primary">
                        </div>
                      </div>
                      <div class="col-md-3">
                        <div class="form-group mt-4 pt-1">
                          <div id="displayDownloadBtn">

                          </div>
                        </div>
                      </div>
                    </div>
                  </form>
                </div>

                <br />
                <table id="senderIdTable" class="table table-striped table-sm table-bordered dt-responsive nowrap"
                  style="width:100%">
                  <tfoot>
                    <tr>
                      <th colspan="4" style="text-align:right">Total:</th>
                      <th colspan="1"></th>
                      <th colspan="1"></th>
                      <th colspan="1"></th>
                    </tr>
                  </tfoot>
                </table>
              </div>

            </div>
          </main>
        </div>
      </div>
    </div>
  </div>

  <!-- Modal -->
  <div id="myModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <h4 class="modal-title">Edit</h4>
          <button type="button" class="close" data-dismiss="modal">&times;</button>
        </div>
        <div class="modal-body">
          <div class="editSenderId">
            <div class="float-left"><b>Sender ID : </b><span id="senderId"></span></div>
            <div class="float-right"><b>Username : </b><span id="userName"></span></div>
            <div class="d-none" id="idOfSenderId"></div>
            <br />
            <br />
            <div class="row">
              <div class="col-md-3 isActive">
                <label for="isActive">isActive</label>
                <select type="text" class="form-control" name="isActive" id="isActive">
                  <option value="Y">Y</option>
                  <option value="N">N</option>
                </select>
              </div>

              <div class="col-md-3 isDefault">
                <label for="isDefault">isDefault</label>
                <select type="text" class="form-control" name="isDefault" id="isDefault">
                  <option value="Y">Y</option>
                  <option value="N">N</option>
                </select>
              </div>

              <div class="col-md-3 isNonDndNumberAllowed">
                <label for="isNonDndNumberAllowed">isNonDndNumberAllowed</label>
                <select type="text" class="form-control" name="isNonDndNumberAllowed" id="isNonDndNumberAllowed">
                  <option value="Y">Y</option>
                  <option value="N">N</option>
                </select>
              </div>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-primary" id="update" data-dismiss="modal">Update</button>
          <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
        </div>
      </div>
    </div>
  </div>


  <div class="footer-div">
    <span class="copy float-left">
      <p id="demo"></p> &copy; All Rights Reserved
    </span>
    <span class="float-right">
      <ul class="social-media-icon">
        <li><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-rss" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-whatsapp" aria-hidden="true"></i></a></li>
      </ul>
    </span>
  </div>
</div>