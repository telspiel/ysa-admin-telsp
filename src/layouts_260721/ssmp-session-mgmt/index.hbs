{{> sideNav }}
<!-- [ Main Content ] start -->
<div class="pcoded-main-container">
  <div class="pcoded-content">
    <!-- [ Main Content ] start -->
    <div class="p-0">
      <div class="p-0 contant-holder">
        <!-- contant holder start -->
        <h3 class="main-head-3">SMPP Session Management</h3>
        <!-- contant holder end -->
        <div class="col-md-12 pt-2 pb-5">
<div id="alert-box" class="alert-box"></div>
          <div class="frm-border d-none">
            <form id="controls-form">
              <div class="row">

                <div class="col-md-4">
                  <div class="form-group">
                    <label for="userName">Search UserD</label>
                    <input type="text" class="form-control" id="userName" placeholder="Search"
                      aria-describedby="add-sender-id">
                  </div>
                </div>
                <div class="col-md-4">
                  <div class="form-group mt-4 pt-2">
                    <input type="submit" value="Search" class="btn btn-primary">
                  </div>
                </div>

              </div>
            </form>
          </div>

          <table id="misTable" class="table table-striped table-sm table-bordered dt-responsive nowrap"
            style="width:100%">
          </table>
          <ul id="misPagination" class="pagination float-right table-pagination">
            <li class="page-item"><a class="page-link" id="pagePrev" href="#">Previous</a></li>
            <li class="page-item"><a class="page-link" id="pageNext" href="#">Next</a></li>
          </ul>
        </div>
      </div>
      <!-- [ Main Content ] end -->
    </div>

  </div>
  <div class="footer-div">
    <span class="copy float-left">
      <p id="demo"></p> &copy; All Rights Reserved
    </span>
    <span class="float-right">
      <ul class="social-media-icon">
        <li><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-rss" aria-hidden="true"></i></a></li>
        <li><a href="#"><i class="fa fa-whatsapp" aria-hidden="true"></i></a></li>
      </ul>
    </span>
  </div>
</div>


<!-- Modal -->
<div id="tableModal" class="modal fade" role="dialog">
  <div class="modal-dialog modal-lg">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">Detailed View</h4>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
      </div>
      <div class="modal-body">
        <h6 class="modal-title rxSessionDetailsList d-none">Rx Session Details List</h6>
        <table class="table table-striped table-sm table-bordered dt-responsive nowrap" id="rxSessionDetailsList">
        </table>
        <h6 class="modal-title txSessionDetailsList d-none">Tx Session Details List</h6>
        <table class="table table-striped table-sm table-bordered dt-responsive nowrap" id="txSessionDetailsList">
        </table>
        <h6 class="modal-title trxSessionDetailsList d-none">TRx Session Details List</h6>
        <table class="table table-striped table-sm table-bordered dt-responsive nowrap" id="trxSessionDetailsList">
        </table>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
